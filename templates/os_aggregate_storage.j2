{% for log_file in rsyslog_client_all_log_files %}
{{ log_file }}
{% endfor %}
{
        copytruncate
        weekly
        missingok
        rotate 14
        compress
        dateext
        maxage 60
        notifempty
        nocreate
        postrotate
            restart rsyslog 2>&1 || true
        endscript
}
